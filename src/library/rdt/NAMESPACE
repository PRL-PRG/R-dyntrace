# Refer to all C routines by their name prefixed by C_
useDynLib(rdt, .registration = TRUE, .fixes = "C_")

export(Rdt)

export(trace.promises.r)
export(trace.promises.file)
export(trace.promises.sql)
export(trace.promises.db)
export(trace.promises.compiled.db)
export(trace.promises.uncompiled.db)
export(trace.promises.both)

export(CONSOLE)
export(FILE)
export(DB)

export(wrap.executor)
export(wrap.session.executor)

export(run.all.vignettes.from.package)
export(run.all.vignettes.from.packages)
export(run.all.vignettes.from.all.packages)

export(RdtCalltimes, RdtFlow)
